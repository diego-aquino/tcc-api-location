{
  "installCommand": "pnpm install --frozen-lockfile --prod=false",
  "buildCommand": null,
  "outputDirectory": "public",
  "trailingSlash": false,
  "functions": {
    "api/**/*.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
