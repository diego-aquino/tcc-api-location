{
  "installCommand": "pnpm install --frozen-lockfile --prod=false",
  "devCommand": "pnpm dev",
  "buildCommand": "pnpm build && mkdir -p public && cp -r dist api",
  "trailingSlash": false,
  "outputDirectory": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
